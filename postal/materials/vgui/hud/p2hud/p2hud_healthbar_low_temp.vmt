UnlitGeneric
{
$baseTexture "vgui\hud\p2hud\p2hud_healthbar_temp"
$translucent 1
$alpha 1
$vertexalpha 1
$ignorez 1
$no_fullbright 1
$vertexcolor 1

"Proxies"
{
	"AnimatedTexture"
	{	
		"animatedTextureVar" "$basetexture"
		"animatedTextureFrameNumVar" "$frame"
		"animatedTextureFrameRate" "25"
	}
	Sine
	{
		sineperiod 0.5
		sinemin 1
		sinemax 10
		resultVar "$color[0]"
	}
}



}
