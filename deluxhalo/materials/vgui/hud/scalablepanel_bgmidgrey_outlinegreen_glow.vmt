UnlitGeneric
{
$translucent 1
$basetexture "VGUI\hud\selected"
$vertexcolor 1
$vertexalpha 1
$no_fullbright 1
$ignorez 1
$additive 0
$nomip 1
$frame 1
Proxies
{
AnimatedTexture
{ 
animatedTextureVar $basetexture 
animatedTextureFrameNumVar $frame
animatedTextureFrameRate 5 //this is the FPS that the texture will animate at
}
}
}