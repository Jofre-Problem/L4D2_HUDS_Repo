UnlitGeneric
{
    $translucent 1
    $basetexture "VGUI/hud/hardcore_health_bar_animated"
    $vertexcolor 1
    $vertexalpha 1
    $no_fullbright 1
    $ignorez 1
    $additive 0
    $nomip 1
    $model 1

    "Proxies"
    {
        "MaterialModifyAnimated"
        {
            "animatedtexturevar" "$basetexture"
            "animatedtextureframenumvar" "$frame"
            "animatedtextureframerate" 20
        }
    }
}
