UnlitGeneric
{
$translucent 1
$basetexture "VGUI\hud\circle_incap_glow"
$vertexcolor 1
$vertexalpha 1
$ignorez 1
$no_fullbright 1

"Proxies"
	{
	"MaterialModifyAnimated"
		{
         		"animatedtexturevar" "$basetexture"
         		"animatedtextureframenumvar" "$frame"
         		"animatedtextureframerate" 30
		}
   	}
}
