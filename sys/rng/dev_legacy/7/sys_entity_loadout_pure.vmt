	{
		srcVar1 		"$tempDIV"
		resultVar 		"$tempDIV"    
	}

	Multiply			// This multiples the fractional part by the number of skins we have (i.e. 0.85714 * 7 = 5.99998)
	{
		srcVar1 		"$tempDIV"
		srcVar2 		"$loadoutsInUse"
		resultVar 		"$tempMULT"    
	}

