UnlitGeneric
{
$baseTexture "vgui\hud\active"
$translucent 1
$alpha 1
$vertexalpha 1
$ignorez 1
$no_fullbright 1
$vertexcolor 1
	
	Proxies
		{
			MaterialModifyAnimated
				{
					animatedtexturevar "$basetexture"
					animatedtextureframenumvar "$frame"
					animatedtextureframerate "27"
				}
		}
}
